<%= simple_form_for(@online_answer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :online_quiz_id %>
    <%= f.input :answer %>
    <%= f.input :xcoor %>
    <%= f.input :ycoor %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
