<%= simple_form_for(@online_quiz_grade) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :online_quiz_id %>
    <%= f.input :online_answer_id %>
    <%= f.input :grade %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
